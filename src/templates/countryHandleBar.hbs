<div class="card">
    {{#each this}}
  <div class="card-img-top">
    <img src="{{flags.svg}}" alt="{{name.official}}" width=120 height=100>
  </div>
  <div class="card-body">
    <h2 class="card-title"> Name: {{name.official}}</h2>
    <p class="card-text"> Capital: {{ capital }}</p>
    <p class="card-text"> Population: {{population}}</p>
     <p class="card-text"> Languages:
    {{#each languages}}
     {{this}}
    {{/each}}
 </p>
  </div>
     {{/each}}
</div>