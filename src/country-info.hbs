
<div>
  {{#each this}}
  <div class=""country-info"">
  <img 
  src={{0.flags.svg}}
  alt={{0.name.official}}
    / >
    <p class="country-title">{{0.name.official}}</p>
  </div>
  <p class="country-capital">Capital: {{0.capital}}</p>
  <p class="country-population">Population: {{0.population}}</p>
    {{/each}}
</div>
